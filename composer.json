{
  "name": "larapacks/table",
  "description": "An HTML table generator for Laravel.",
  "keywords": ["html", "generator", "table", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "orchestra/testbench": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Larapacks\\Table\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapacks\\Table\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
